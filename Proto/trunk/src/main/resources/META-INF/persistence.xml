<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="eduobject" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>edu.data.Disposabilities</class>
    <class>edu.data.Subjects</class>
    <class>edu.data.CoursesGroups</class>
    <class>edu.data.Staff</class>
    <class>edu.data.Teachers</class>
    <class>edu.data.Students</class>
    <class>edu.data.Departments</class>
    <class>edu.data.GroupsStudents</class>
    <class>edu.data.Groups</class>
    <class>edu.data.TeachersCourses</class>
    <class>edu.data.Courses</class>
    <class>edu.data.TeachersSubjects</class>
    <class>edu.data.Participants</class>
    <class>edu.data.Scheduler</class>
    <class>edu.data.TeachersResults</class>
    <class>edu.data.Majors</class>
    <class>edu.data.Classes</class>
    <class>edu.data.Results</class>
    <class>edu.data.Schools</class>
    <class>edu.data.CoursesLocations</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:Cache://localhost:1972/USER"/>
      <property name="javax.persistence.jdbc.user" value="_SYSTEM"/>
      <property name="javax.persistence.jdbc.driver" value="com.intersys.jdbc.CacheDriver"/>
      <property name="javax.persistence.jdbc.password" value="SYS"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
      <property name="eclipselink.logging.file" value="D:\tools\test.log"/>
      <property name="eclipselink.logging.level" value="ALL"/>
      <property name="eclipselink.target-database" value="Database"/>
    </properties>
  </persistence-unit>
</persistence>
